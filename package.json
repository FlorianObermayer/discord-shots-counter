{
  "name": "discord-shots-counter",
  "private": true,
  "version": "1.0.0",
  "description": "Shots Counter for Discord",
  "main": "src/app.js",
  "type": "module",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "build:prod": "NODE_ENV=production tsup src/app.ts --minify",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "register:prod": "NODE_ENV=production node src/commands.js",
    "start": "NODE_ENV=development tsx watch --env-file=.env.development src/app.ts",
    "register": "NODE_ENV=development node --env-file=.env.development src/commands.ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "ngrok": "ngrok http 3000"
  },
  "author": "Florian Obermayer",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "axios": "^1.9.0",
    "discord-api-types": "^0.38.1",
    "discord-interactions": "^4.0.0",
    "discord.js": "^14.19.1",
    "express": "^5.1.0",
    "ffmpeg-static": "^5.2.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  }
}
