{
  "name": "discord-shots-counter",
  "private": true,
  "version": "1.0.0",
  "description": "Shots Counter for Discord",
  "main": "src/app.js",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start:prod": "NODE_ENV=production node src/app.js",
    "register:prod": "NODE_ENV=production node src/commands.js",
    "start": "NODE_ENV=development nodemon --env-file=.env.development src/app.js",
    "register": "NODE_ENV=development node --env-file=.env.development src/commands.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "ngrok": "ngrok http 3000"
  },
  "author": "Shay DeWael",
  "license": "MIT",
  "dependencies": {
    "discord-interactions": "^4.0.0",
    "express": "^4.18.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.25.1",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0"
  }
}
